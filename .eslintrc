{
  parser: "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/no-autofocus": [ 0, {
            "ignoreNonDOM": true
        }],
    "react/prop-types": 0,
    "indent": ["error", 2]
  }
}
